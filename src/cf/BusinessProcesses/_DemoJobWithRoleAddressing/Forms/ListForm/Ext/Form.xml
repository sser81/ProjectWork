	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	BusinessProcessDemoTaskWithRoleAddressing.Ref,
	BusinessProcessDemoTaskWithRoleAddressing.DeletionMark,
	BusinessProcessDemoTaskWithRoleAddressing.Number,
	BusinessProcessDemoTaskWithRoleAddressing.Date,
	BusinessProcessDemoTaskWithRoleAddressing.Completed,
	BusinessProcessDemoTaskWithRoleAddressing.HeadTask,
	BusinessProcessDemoTaskWithRoleAddressing.Started,
	BusinessProcessDemoTaskWithRoleAddressing.Author,
	BusinessProcessDemoTaskWithRoleAddressing.CompletedOn,
	BusinessProcessDemoTaskWithRoleAddressing.Description,
	BusinessProcessDemoTaskWithRoleAddressing.MainAddressingObject,
	BusinessProcessDemoTaskWithRoleAddressing.AdditionalAddressingObject,
	BusinessProcessDemoTaskWithRoleAddressing.Performer,
	BusinessProcessDemoTaskWithRoleAddressing.PerformerRole,
	BusinessProcessDemoTaskWithRoleAddressing.TaskDueDate,
	BusinessProcessDemoTaskWithRoleAddressing.SubjectOf,
	BusinessProcessDemoTaskWithRoleAddressing.TaskPerformersGroup,
	CASE
		WHEN BusinessProcessDemoTaskWithRoleAddressing.State = VALUE(Enum.BusinessProcessStates.Suspended)
			THEN TRUE
		ELSE FALSE
	END AS Suspended,
	BusinessProcessDemoTaskWithRoleAddressing.CheckExecution,
	BusinessProcessDemoTaskWithRoleAddressing.Supervisor,
	BusinessProcessDemoTaskWithRoleAddressing.AdditionalAddressingObjectSupervisor,
	BusinessProcessDemoTaskWithRoleAddressing.MainAddressingObjectSupervisor,
	BusinessProcessDemoTaskWithRoleAddressing.SupervisorRole
FROM
	BusinessProcess._DemoJobWithRoleAddressing AS BusinessProcessDemoTaskWithRoleAddressing
WHERE
	(&amp;Performer IS NULL 
			OR BusinessProcessDemoTaskWithRoleAddressing.Performer = &amp;Performer
			OR TRUE IN
				(SELECT
					TRUE
				FROM
					InformationRegister.TaskPerformers AS TaskPerformers
				WHERE
					BusinessProcessDemoTaskWithRoleAddressing.PerformerRole = TaskPerformers.PerformerRole
					AND BusinessProcessDemoTaskWithRoleAddressing.MainAddressingObject = TaskPerformers.MainAddressingObject
					AND BusinessProcessDemoTaskWithRoleAddressing.AdditionalAddressingObject = TaskPerformers.AdditionalAddressingObject
					AND TaskPerformers.Performer = &amp;Performer))</QueryText>
				<MainTable>BusinessProcess._DemoJobWithRoleAddressing</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
