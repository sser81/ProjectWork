			<UseAlways>
				<Field>List.IsFolder</Field>
				<Field>List.Ref</Field>
				<Field>List.User</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	AccessGroups.Ref,
	AccessGroups.DeletionMark,
	AccessGroups.Predefined,
	AccessGroups.Parent,
	AccessGroups.IsFolder,
	AccessGroups.Description,
	AccessGroups.Profile,
	AccessGroups.User,
	AccessGroups.Comment,
	AccessGroups.EmployeeResponsible
FROM
	Catalog.AccessGroups AS AccessGroups
WHERE
	CASE
			WHEN &amp;Profile = VALUE(Catalog.AccessGroupProfiles.EmptyRef)
				THEN TRUE
			ELSE AccessGroups.Profile = &amp;Profile
		END</QueryText>
				<MainTable>Catalog.AccessGroups</MainTable>
