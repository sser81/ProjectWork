	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.DeletionMark</Field>
				<Field>List.Description</Field>
				<Field>List.FileName</Field>
				<Field>List.IsFolder</Field>
				<Field>List.IsReport</Field>
				<Field>List.ObjectName</Field>
				<Field>List.Publication</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Table.Ref,
	Table.DeletionMark,
	Table.Description,
	Table.Version,
	Table.EmployeeResponsible,
	Table.Publication,
	Table.Kind,
	Table.ObjectName,
	Table.IsFolder,
	CASE
		WHEN Table.Kind IN (&amp;AddlReportsKinds)
			THEN TRUE
		ELSE FALSE
	END AS IsReport,
	Table.FileName
FROM
	Catalog.AdditionalReportsAndDataProcessors AS Table
WHERE
	CASE
			WHEN Table.IsFolder
				THEN TRUE
			WHEN &amp;PublicationFilter = UNDEFINED
				THEN TRUE
			WHEN &amp;PublicationFilter = 1
				THEN Table.Publication IN (&amp;AllPublicationsExceptDisabled)
			ELSE Table.Publication = &amp;PublicationFilter
		END
	AND CASE
			WHEN Table.IsFolder
				THEN TRUE
			WHEN &amp;KindFilter = UNDEFINED
				THEN TRUE
			WHEN &amp;KindFilter = 1
				THEN Table.Kind IN (&amp;AddlReportsKinds)
			WHEN &amp;KindFilter = 2
				THEN NOT Table.Kind IN (&amp;AddlReportsKinds)
			ELSE Table.Kind = &amp;KindFilter
		END</QueryText>
				<MainTable>Catalog.AdditionalReportsAndDataProcessors</MainTable>
