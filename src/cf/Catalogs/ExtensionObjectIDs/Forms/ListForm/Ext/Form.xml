			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.DeletionMark</Field>
				<Field>List.Description</Field>
				<Field>List.FutureDeletionMark</Field>
				<Field>List.Name</Field>
				<Field>List.Parent</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	IDs.Ref,
	IDs.DataVersion,
	IDs.DeletionMark,
	CASE
		WHEN SUBSTRING(IDs.Name, 0, 7) = "Delete"
			THEN TRUE
		ELSE FALSE
	END AS FutureDeletionMark,
	IDs.Predefined,
	IDs.Parent,
	IDs.Description,
	IDs.CollectionOrder,
	IDs.Name,
	IDs.Synonym,
	IDs.FullName,
	IDs.FullSynonym,
	IDs.NoData,
	IDs.EmptyRefValue,
	IDs.MetadataObjectKey
FROM
	Catalog.ExtensionObjectIDs AS IDs</QueryText>
				<MainTable>Catalog.ExtensionObjectIDs</MainTable>
