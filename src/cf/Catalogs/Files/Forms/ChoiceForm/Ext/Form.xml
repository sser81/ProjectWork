	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
				<Field>List.SignedWithDS</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	Files.Ref,
	Files.DeletionMark,
	Files.Predefined,
	Files.Author,
	Files.CreationDate,
	Files.StoreVersions,
	Files.BeingEditedBy,
	Files.CurrentVersion,
	Files.PictureIndex,
	Files.LongDesc,
	Files.Description,
	Files.FileOwner,
	Files.SignedWithDS,
	Files.Encrypted,
	CASE
		WHEN Files.SignedWithDS
				AND Files.Encrypted
			THEN 2
		WHEN Files.Encrypted
			THEN 1
		WHEN Files.SignedWithDS
			THEN 0
		ELSE -1
	END AS SignedEncryptedPictureNumber,
	Files.Extension
FROM
	Catalog.Files AS Files
WHERE
	Files.FileOwner = &amp;Owner
	AND Files.DeletionMark = FALSE</QueryText>
				<MainTable>Catalog.Files</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
