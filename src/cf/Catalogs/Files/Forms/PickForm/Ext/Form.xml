			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Files.Ref,
	Files.DeletionMark,
	Files.Predefined,
	Files.Description,
	Files.Author,
	Files.CreationDate,
	Files.StoreVersions,
	Files.BeingEditedBy,
	Files.CurrentVersion,
	Files.PictureIndex,
	Files.LongDesc,
	Files.FileOwner
FROM
	Catalog.Files AS Files
WHERE
	Files.FileOwner = &amp;Owner
	AND Files.DeletionMark = FALSE</QueryText>
