				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogSourceDocumentsOriginalsStates.Ref AS Ref,
	CatalogSourceDocumentsOriginalsStates.DeletionMark AS DeletionMark,
	CatalogSourceDocumentsOriginalsStates.Code AS Code,
	CatalogSourceDocumentsOriginalsStates.Description AS Description,
	CatalogSourceDocumentsOriginalsStates.LongDesc AS LongDesc,
	CatalogSourceDocumentsOriginalsStates.AddlOrderingAttribute AS AddlOrderingAttribute,
	CatalogSourceDocumentsOriginalsStates.Predefined AS Predefined,
	CatalogSourceDocumentsOriginalsStates.PredefinedDataName AS PredefinedDataName,
	CASE
		WHEN CatalogSourceDocumentsOriginalsStates.Ref = VALUE(Catalog.SourceDocumentsOriginalsStates.OriginalReceived)
			THEN 1
		WHEN CatalogSourceDocumentsOriginalsStates.Ref = VALUE(Catalog.SourceDocumentsOriginalsStates.FormPrinted)
			THEN 0
		WHEN CatalogSourceDocumentsOriginalsStates.DeletionMark
			THEN 4
		ELSE 3
	END AS Picture
FROM
	Catalog.SourceDocumentsOriginalsStates AS CatalogSourceDocumentsOriginalsStates
WHERE
	NOT CatalogSourceDocumentsOriginalsStates.Ref = VALUE(Catalog.SourceDocumentsOriginalsStates.OriginalsNotAll)
	AND NOT CatalogSourceDocumentsOriginalsStates.DeletionMark</QueryText>
