	CatalogTypesOfLimitsForEmployees.OneTime AS OneTime,
	CatalogTypesOfLimitsForEmployees.Predefined AS Predefined,
	CatalogTypesOfLimitsForEmployees.PredefinedDataName AS PredefinedDataName,
	CASE
		WHEN CatalogTypesOfLimitsForEmployees.CountLimitControl
			THEN 1
		WHEN CatalogTypesOfLimitsForEmployees.CheckLimitControl
			THEN 2
		ELSE 0
	END AS ResourceType
