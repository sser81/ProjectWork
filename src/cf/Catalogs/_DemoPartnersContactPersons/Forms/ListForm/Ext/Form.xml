	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT ALLOWED
	-1 AS ExternalAccessPicNum,
	NOT ExternalUsers.Ref IS NULL
		AND NOT ExternalUsers.Invalid
		AND NOT ExternalUsers.DeletionMark AS ExternalAccess,
	Catalog_DemoPartnersContactPersons.Ref AS Ref,
	Catalog_DemoPartnersContactPersons.Description AS Description
FROM
	Catalog._DemoPartnersContactPersons AS Catalog_DemoPartnersContactPersons
		{LEFT JOIN Catalog.ExternalUsers AS ExternalUsers
		ON Catalog_DemoPartnersContactPersons.Ref = ExternalUsers.AuthorizationObject}</QueryText>
				<MainTable>Catalog._DemoPartnersContactPersons</MainTable>
