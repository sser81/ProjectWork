	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.IconIndex</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DemoMainChartOfAccounts.Ref,
	
	DemoMainChartOfAccounts.DeletionMark,
	DemoMainChartOfAccounts.Predefined,
	
	DemoMainChartOfAccounts.Parent,
	DemoMainChartOfAccounts.Code,
	DemoMainChartOfAccounts.Description,
	DemoMainChartOfAccounts.Order,
	DemoMainChartOfAccounts.Type,
	DemoMainChartOfAccounts.OffBalance,
	DemoMainChartOfAccounts.Currency1,
	DemoMainChartOfAccounts.Quantitative,
	DemoMainChartOfAccounts.ExtDimensionTypes,
	DemoMainChartOfAccounts.PredefinedDataName,

	ExtDimension1.ExtDimensionType AS ExtDimension1,
	ExtDimension2.ExtDimensionType AS ExtDimension2,
	ExtDimension3.ExtDimensionType AS ExtDimension3,

	CASE
		WHEN DemoMainChartOfAccounts.Type = VALUE(AccountType.Active)
			THEN "A"
		ELSE
			CASE
				WHEN DemoMainChartOfAccounts.Type = VALUE(AccountType.Passive)
					THEN "P"
				ELSE "AP"
			END
	END AS KindString,
	
	CASE 
		WHEN DemoMainChartOfAccounts.DeletionMark THEN
			CASE 
				WHEN DemoMainChartOfAccounts.Predefined THEN 3
				ELSE 2
			END
		ELSE
			CASE 
				WHEN DemoMainChartOfAccounts.Predefined THEN 1
				ELSE 0
			END
	END AS IconIndex
	
FROM
	ChartOfAccounts._DemoMain AS DemoMainChartOfAccounts
	
LEFT JOIN
	ChartOfAccounts._DemoMain.ExtDimensionTypes AS ExtDimension1
ON
	DemoMainChartOfAccounts.Ref = ExtDimension1.Ref AND ExtDimension1.LineNumber = 1
	
LEFT JOIN
	ChartOfAccounts._DemoMain.ExtDimensionTypes AS ExtDimension2
ON
	DemoMainChartOfAccounts.Ref = ExtDimension2.Ref AND ExtDimension2.LineNumber = 2
	
LEFT JOIN
	ChartOfAccounts._DemoMain.ExtDimensionTypes AS ExtDimension3
ON
	DemoMainChartOfAccounts.Ref = ExtDimension3.Ref AND ExtDimension3.LineNumber = 3
</QueryText>
				<MainTable>ChartOfAccounts._DemoMain</MainTable>
