			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.BeingEditedBy</Field>
				<Field>List.CurrentUserEditsFile</Field>
				<Field>List.Ref</Field>
				<Field>List.StoreVersions</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	FilesInfo.BeingEditedBy AS BeingEditedBy,
	FilesInfo.Size / 1024 AS Size,
	FilesInfo.Author AS Author,
	FilesInfo.CreationDate AS CreationDate,
	CASE
		WHEN FilesInfo.DeletionMark
			THEN FilesInfo.PictureIndex + 1
		ELSE FilesInfo.PictureIndex
	END AS PictureIndex,
	FilesInfo.Description AS Description,
	FilesInfo.LongDesc AS LongDesc,
	FilesInfo.File AS Ref,
	FilesInfo.DeletionMark AS DeletionMark,
	CASE
		WHEN FilesInfo.BeingEditedBy = &amp;CurrentUser
			THEN TRUE
		ELSE FALSE
	END AS CurrentUserEditsFile,
	FilesInfo.FileOwner AS FileOwner,
	FilesInfo.Extension AS Extension,
	FilesInfo.StoreVersions AS StoreVersions,
	FilesInfo.SignedEncryptedPictureNumber AS SignedEncryptedPictureNumber,
	FilesInfo.IsInternal AS IsInternal
FROM
	InformationRegister.FilesInfo AS FilesInfo
WHERE
	NOT FilesInfo.File IS NULL</QueryText>
				<MainTable>InformationRegister.FilesInfo</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
	</Attributes>
