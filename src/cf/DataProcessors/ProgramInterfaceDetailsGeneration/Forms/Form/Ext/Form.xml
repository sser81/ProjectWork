	<Title>
		<v8:item>
			<v8:lang>en</v8:lang>
			<v8:content>Generating API details</v8:content>
		</v8:item>
	</Title>
	<WindowOpeningMode>LockOwnerWindow</WindowOpeningMode>
	<AutoSaveDataInSettings>Use</AutoSaveDataInSettings>
	<AutoTitle>false</AutoTitle>
	<AutoCommandBar name="FormCommandBar" id="-1">
		<ChildItems>
			<Button name="FormPrepare" id="1">
				<Type>CommandBarButton</Type>
				<DefaultButton>true</DefaultButton>
				<CommandName>Form.Command.Prepare</CommandName>
				<Title>
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>Prepare</v8:content>
					</v8:item>
				</Title>
				<ExtendedTooltip name="FormPrepareExtendedTooltip" id="2"/>
			</Button>
			<Button name="FormOpenWarningsList" id="6">
				<Type>CommandBarButton</Type>
				<CommandName>Form.Command.OpenWarningsList</CommandName>
				<Title>
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>List of warnings</v8:content>
					</v8:item>
				</Title>
				<ExtendedTooltip name="OpenWarningListFormExtendedTooltip" id="7"/>
			</Button>
		</ChildItems>
	</AutoCommandBar>
	<Events>
		<Event name="OnOpen">OnOpen</Event>
	</Events>
	<ChildItems>
		<InputField name="PathToFile" id="3">
			<DataPath>Object.PathToFile</DataPath>
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>File with documentation</v8:content>
				</v8:item>
			</Title>
			<ToolTip>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Path to file to save API details to</v8:content>
				</v8:item>
			</ToolTip>
			<ChoiceButton>true</ChoiceButton>
			<OpenButton>true</OpenButton>
			<ContextMenu name="PathToFileContextMenu" id="4"/>
			<ExtendedTooltip name="PathToFileExtendedTooltip" id="5"/>
			<Events>
				<Event name="StartChoice">PathToFileStartChoice</Event>
				<Event name="Opening">PathToFileOpening</Event>
			</Events>
		</InputField>
		<UsualGroup name="GroupDumpDirectory" id="20">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Export directory</v8:content>
				</v8:item>
			</Title>
			<Behavior>Usual</Behavior>
			<Representation>None</Representation>
			<ShowTitle>false</ShowTitle>
			<ExtendedTooltip name="GroupDumpDirectoryExtendedTooltip" id="21"/>
			<ChildItems>
				<InputField name="DumpDirectory" id="11">
					<DataPath>Object.DumpDirectory</DataPath>
					<Title>
						<v8:item>
							<v8:lang>en</v8:lang>
							<v8:content>File export directory</v8:content>
						</v8:item>
					</Title>
					<ToolTip>
						<v8:item>
							<v8:lang>en</v8:lang>
							<v8:content>Path to a directory with exported configuration files. If not specified, files will be exported to a temporary directory automatically.</v8:content>
						</v8:item>
					</ToolTip>
					<ToolTipRepresentation>Button</ToolTipRepresentation>
					<ChoiceButton>true</ChoiceButton>
					<ContextMenu name="DumpDirectoryContextMenu" id="12"/>
					<ExtendedTooltip name="DumpDirectoryExtendedTooltip" id="13"/>
					<Events>
						<Event name="StartChoice">DumpDirectoryStartChoice</Event>
					</Events>
				</InputField>
				<LabelField name="VersionLanguage" id="17">
					<DataPath>Object.VersionLanguage</DataPath>
					<TitleLocation>None</TitleLocation>
					<HorizontalAlign>Right</HorizontalAlign>
					<GroupHorizontalAlign>Right</GroupHorizontalAlign>
					<AutoMaxWidth>false</AutoMaxWidth>
					<MaxWidth>6</MaxWidth>
					<ContextMenu name="VersionLanguageContextMenu" id="18"/>
					<ExtendedTooltip name="VersionLanguageExtendedTooltip" id="19"/>
				</LabelField>
			</ChildItems>
		</UsualGroup>
		<LabelDecoration name="DecorationNote" id="8">
			<Width>50</Width>
			<TextColor>style:NoteText</TextColor>
			<Title formatted="false">
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Generates documentation by application interface procedures and functions in common configuration modules that are included in analyzed subsystems. Subsystems list is set in SubsystemsWithApplicationInterface processing module function.</v8:content>
				</v8:item>
			</Title>
			<ContextMenu name="DecorationNoteContextMenu" id="9"/>
			<ExtendedTooltip name="DecorationNoteExtendedTooltip" id="10"/>
		</LabelDecoration>
		<InputField name="SubsystemsBeingAnalyzed" id="14">
			<DataPath>Object.SubsystemsBeingAnalyzed</DataPath>
			<ContextMenu name="SubsystemsBeingAnalyzedContextMenu" id="15"/>
			<ExtendedTooltip name="SubsystemsBeingAnalyzedExtendedTooltip" id="16"/>
			<Events>
				<Event name="StartChoice">SubsystemsBeingAnalyzedStartChoice</Event>
			</Events>
		</InputField>
	</ChildItems>
	<Attributes>
		<Attribute name="Object" id="1">
			<Type>
				<v8:Type>cfg:DataProcessorObject.ProgramInterfaceDetailsGeneration</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Save>
				<Field>Object.DumpDirectory</Field>
				<Field>Object.PathToFile</Field>
			</Save>
		</Attribute>
	</Attributes>
	<Commands>
		<Command name="Prepare" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Prepare</v8:content>
				</v8:item>
			</Title>
			<ToolTip>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Prepare</v8:content>
				</v8:item>
			</ToolTip>
			<Action>Prepare</Action>
			<CurrentRowUse>DontUse</CurrentRowUse>
		</Command>
		<Command name="OpenWarningsList" id="2">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>List of warnings</v8:content>
				</v8:item>
			</Title>
			<ToolTip>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Show the list of warnings on generating API details</v8:content>
				</v8:item>
			</ToolTip>
			<Action>OpenWarningsList</Action>
			<CurrentRowUse>DontUse</CurrentRowUse>
		</Command>
	</Commands>
</Form>