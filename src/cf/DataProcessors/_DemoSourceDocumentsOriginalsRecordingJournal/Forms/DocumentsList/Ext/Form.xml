	<Attributes>
		<Attribute name="DocumentsList" id="2">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Document list</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>DocumentsList.FromOutside</Field>
				<Field>DocumentsList.OverallState</Field>
				<Field>DocumentsList.PictureField</Field>
				<Field>DocumentsList.Ref</Field>
				<Field>DocumentsList.RefType</Field>
				<Field>DocumentsList.SourceDocument</Field>
				<Field>DocumentsList.SourceDocumentOriginalState</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	SourceDocumentsOriginalsStates.OverallState AS OverallState,
	CASE
		WHEN SourceDocumentsOriginalsStates.ChangeAuthor = VALUE(Catalog.Users.EmptyRef)
			THEN ""
		ELSE SourceDocumentsOriginalsStates.ChangeAuthor
	END AS ChangeAuthor,
	SourceDocumentsOriginalsStates.LastChangeDate AS LastChangeDate,
	DocumentsRegistry.IBDocumentDate AS IBDocumentDate,
	DocumentsRegistry.IBDocumentNumber AS IBDocumentNumber,
	DocumentsRegistry.Organization AS Organization,
	DocumentsRegistry.Counterparty AS Counterparty,
	DocumentsRegistry.SourceDocumentDate AS SourceDocumentDate,
	DocumentsRegistry.SourceDocumentNumber AS SourceDocumentNumber,
	DocumentsRegistry.Ref AS Ref,
	CASE
		WHEN SourceDocumentsOriginalsStates.OverallState = TRUE
				OR SourceDocumentsOriginalsStates.OverallState IS NULL
			THEN VALUETYPE(DocumentsRegistry.Ref)
		ELSE ""
	END AS DocumentKind,
	DocumentsRegistry.RefType AS RefType,
	CASE
		WHEN SourceDocumentsOriginalsStates.OverallState = TRUE
				OR SourceDocumentsOriginalsStates.OverallState IS NULL
			THEN 0
		WHEN SourceDocumentsOriginalsStates.OverallState = FALSE
				AND SourceDocumentsOriginalsStates.ExternalForm = TRUE
			THEN 2
		ELSE 1
	END AS PictureField,
	SourceDocumentsOriginalsStates.SourceDocument AS SourceDocument,
	DocumentsRegistry.Partner AS Partner,
	SourceDocumentsOriginalsStates.ExternalForm AS FromOutside,
	DocumentsRegistry.Sum AS Sum,
	DocumentsRegistry.Currency AS Currency,
	DocumentsRegistry.Counterparty.TIN AS CounterpartyTIN,
	SourceDocumentsOriginalsStates.State AS SourceDocumentOriginalState,
	0 AS IndentField1,
	CASE
		WHEN SourceDocumentsOriginalsStates.State = VALUE(Catalog.SourceDocumentsOriginalsStates.OriginalReceived)
			THEN 1
		ELSE 0
	END AS StateOriginalReceived,
	SourceDocumentsOriginalsStates.SourceDocumentPresentation AS SourceDocumentPresentation
FROM
	InformationRegister._DemoDocumentsRegistry AS DocumentsRegistry
		LEFT JOIN InformationRegister.SourceDocumentsOriginalsStates AS SourceDocumentsOriginalsStates
		ON (SourceDocumentsOriginalsStates.Owner = DocumentsRegistry.Ref)
WHERE
	DocumentsRegistry.Posted
{WHERE
	((DocumentsRegistry.IBDocumentDate &gt;= &amp;BeginOfPeriod
			OR &amp;BeginOfPeriod = DATETIME(1, 1, 1))
			AND (DocumentsRegistry.IBDocumentDate &lt;= &amp;EndOfPeriod
				OR &amp;EndOfPeriod = DATETIME(1, 1, 1)))}</QueryText>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
