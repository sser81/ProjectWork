	DocumentDemoSaleOfGoods.Comment AS Comment,
	DocumentDemoSaleOfGoods.Goods.(
		Ref AS Ref,
		LineNumber AS LineNumber,
		Products AS Products,
		Count AS Count,
		Price AS Price,
		DocumentInflow AS DocumentInflow,
