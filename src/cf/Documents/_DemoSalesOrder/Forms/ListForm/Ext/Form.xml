	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Document_DemoSalesOrder.Ref AS Ref,
	Document_DemoSalesOrder.DeletionMark AS DeletionMark,
	Document_DemoSalesOrder.Number AS Number,
	Document_DemoSalesOrder.Date AS Date,
	Document_DemoSalesOrder.Posted AS Posted,
	Document_DemoSalesOrder.Organization,
	Document_DemoSalesOrder.Partner,
	Document_DemoSalesOrder.Counterparty,
	Document_DemoSalesOrder.Contract,
	Document_DemoSalesOrder.Currency,
	Document_DemoSalesOrder.OrderStatus,
	Document_DemoSalesOrder.DocumentAmount,
	Document_DemoSalesOrder.DeliveryAddressString AS DeliveryAddress,
	Document_DemoSalesOrder.DeliveryCountry,
	Document_DemoSalesOrder.DeliveryState,
	Document_DemoSalesOrder.DestinationCity,
	Document_DemoSalesOrder.EmailString AS Email,
	Document_DemoSalesOrder.DeliveryDate,
	Document_DemoSalesOrder.ServerDomainName,
	Document_DemoSalesOrder.DeleteOrderClosed,
	Document_DemoSalesOrder.Comment,
	Document_DemoSalesOrder.EmployeeResponsible,
	Document_DemoSalesOrder.ProformaInvoices,
	Document_DemoSalesOrder.PartnersAndContactPersons,
	Document_DemoSalesOrder.ContactInformation,
	Document_DemoSalesOrder.PointInTime AS PointInTime,
	CASE
		WHEN FilesExist.HasFiles IS NULL THEN
			0
		WHEN FilesExist.HasFiles THEN
			1
		ELSE
			0
	END AS HasFiles
FROM
	Document._DemoSalesOrder AS Document_DemoSalesOrder
	LEFT JOIN InformationRegister.FilesExist AS FilesExist
		ON Document_DemoSalesOrder.Ref = FilesExist.ObjectWithFiles
</QueryText>
