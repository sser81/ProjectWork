	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	AccessKeysForRegisters.Register AS Register,
	AccessKeysForRegisters.AccessOption AS AccessOption,
	AccessKeysForRegisters.Field1 AS Field1,
	AccessKeysForRegisters.Field2 AS Field2,
	AccessKeysForRegisters.Field3 AS Field3,
	AccessKeysForRegisters.Field4 AS Field4,
	AccessKeysForRegisters.Field5 AS Field5,
	AccessKeysForRegisters.AccessKey AS AccessKey,
	CASE
		WHEN AccessKeysForRegisters.AccessOption = 0
			THEN FALSE
		ELSE AccessKeysForRegisters.AccessOption - (CAST(AccessKeysForRegisters.AccessOption / 2 - 0.5 AS NUMBER(15, 0))) * 2 = 1
	END AS ForExternalUsers,
	CASE
		WHEN AccessKeysForRegisters.AccessOption = 0
			THEN 0
		WHEN AccessKeysForRegisters.AccessOption / 64 &gt;= 1
			THEN (CAST(AccessKeysForRegisters.AccessOption / 2 - 0.5 AS NUMBER(15, 0))) - (CAST(AccessKeysForRegisters.AccessOption / 64 - 0.5 AS NUMBER(15, 0))) * 32
		ELSE CAST(AccessKeysForRegisters.AccessOption / 2 - 0.5 AS NUMBER(15, 0))
	END AS FieldsInJoinCount,
	CASE
		WHEN AccessKeysForRegisters.AccessOption / 64 &gt;= 1
			THEN CAST(AccessKeysForRegisters.AccessOption / 64 - 0.5 AS NUMBER(15, 0))
		ELSE 0
	END AS FieldsSetInJoin
FROM
	InformationRegister.AccessKeysForRegisters AS AccessKeysForRegisters</QueryText>
				<MainTable>InformationRegister.AccessKeysForRegisters</MainTable>
