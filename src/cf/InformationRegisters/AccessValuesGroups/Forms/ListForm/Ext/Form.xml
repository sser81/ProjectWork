	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.AccessValue</Field>
				<Field>List.DataGroup</Field>
				<Field>List.DefaultPicture</Field>
				<Field>List.PictureNumberAccessValues</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CASE
		WHEN VALUETYPE(AccessValuesGroups.AccessValue) = TYPE(Catalog.Users)
			THEN ISNULL(InfoRecordAccessVal.NumberOfStatePicture, 0) - 1
		WHEN VALUETYPE(AccessValuesGroups.AccessValue) = TYPE(Catalog.UserGroups)
			THEN CASE
					WHEN CAST(AccessValuesGroups.AccessValue AS Catalog.UserGroups).DeletionMark
						THEN 2
					ELSE 3
				END
		WHEN VALUETYPE(AccessValuesGroups.AccessValue) = TYPE(Catalog.ExternalUsers)
			THEN ISNULL(InfoRecordAccessVal.NumberOfStatePicture, 0) - 1
		WHEN VALUETYPE(AccessValuesGroups.AccessValue) = TYPE(Catalog.ExternalUsersGroups)
			THEN CASE
					WHEN CAST(AccessValuesGroups.AccessValue AS Catalog.ExternalUsersGroups).DeletionMark
						THEN 8
					ELSE 9
				END
		ELSE -1
	END AS PictureNumberAccessValues,
	VALUETYPE(AccessValuesGroups.AccessValue) AS AccessValueType,
	AccessValuesGroups.AccessValue AS AccessValue,
	CASE
		WHEN VALUETYPE(AccessValuesGroups.AccessValuesGroup) = TYPE(Catalog.Users)
			THEN ISNULL(GroupAccessValuesInfoRecrods.NumberOfStatePicture, 0) - 1
		WHEN VALUETYPE(AccessValuesGroups.AccessValuesGroup) = TYPE(Catalog.UserGroups)
			THEN CASE
					WHEN CAST(AccessValuesGroups.AccessValuesGroup AS Catalog.UserGroups).DeletionMark
						THEN 2
					ELSE 3
				END
		WHEN VALUETYPE(AccessValuesGroups.AccessValuesGroup) = TYPE(Catalog.ExternalUsers)
			THEN ISNULL(GroupAccessValuesInfoRecrods.NumberOfStatePicture, 0) - 1
		WHEN VALUETYPE(AccessValuesGroups.AccessValuesGroup) = TYPE(Catalog.ExternalUsersGroups)
			THEN CASE
					WHEN CAST(AccessValuesGroups.AccessValuesGroup AS Catalog.ExternalUsersGroups).DeletionMark
						THEN 8
					ELSE 9
				END
		ELSE -1
	END AS PictureNumberAccessValuesGroups,
	AccessValuesGroups.AccessValuesGroup AS AccessValuesGroup,
	AccessValuesGroups.DataGroup AS DataGroup
FROM
	InformationRegister.AccessValuesGroups AS AccessValuesGroups
		LEFT JOIN InformationRegister.UsersInfo AS InfoRecordAccessVal
		ON (InfoRecordAccessVal.User = AccessValuesGroups.AccessValue)
		LEFT JOIN InformationRegister.UsersInfo AS GroupAccessValuesInfoRecrods
		ON (GroupAccessValuesInfoRecrods.User = AccessValuesGroups.AccessValuesGroup)</QueryText>
				<MainTable>InformationRegister.AccessValuesGroups</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>DataGroup</dcsset:field>
							<dcsset:orderType>Desc</dcsset:orderType>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
