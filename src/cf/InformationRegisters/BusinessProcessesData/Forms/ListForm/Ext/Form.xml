	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Author</Field>
				<Field>List.BusinessProcess</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	InformationRegisterBusinessProcessesData.Owner AS BusinessProcess,
	InformationRegisterBusinessProcessesData.Number,
	InformationRegisterBusinessProcessesData.Date,
	InformationRegisterBusinessProcessesData.Completed,
	InformationRegisterBusinessProcessesData.Started,
	InformationRegisterBusinessProcessesData.Author,
	InformationRegisterBusinessProcessesData.CompletedOn,
	InformationRegisterBusinessProcessesData.Description,
	InformationRegisterBusinessProcessesData.DeletionMark,
	InformationRegisterBusinessProcessesData.State,
	CASE
		WHEN InformationRegisterBusinessProcessesData.DeletionMark
			THEN 6
		WHEN InformationRegisterBusinessProcessesData.Completed = TRUE
			THEN 5
		WHEN InformationRegisterBusinessProcessesData.Started = TRUE
			THEN 3
		WHEN ProcessesToStart.State = VALUE(Enum.ProcessesStatesForStart.ReadyToStart)
				AND ProcessesToStart.DeferredStartDate &lt;&gt; DATETIME(1, 1, 1)
			THEN 1
		WHEN ProcessesToStart.State = VALUE(Enum.ProcessesStatesForStart.StartCanceled)
			THEN 2
		ELSE 0
	END AS RowPicture,
	CASE
		WHEN ProcessesToStart.State IS NULL 
			THEN FALSE
		WHEN ProcessesToStart.State = VALUE(Enum.ProcessesStatesForStart.ReadyToStart)
			THEN TRUE
		ELSE FALSE
	END AS Defer
FROM
	InformationRegister.BusinessProcessesData AS InformationRegisterBusinessProcessesData
		LEFT JOIN InformationRegister.ProcessesToStart AS ProcessesToStart
		ON InformationRegisterBusinessProcessesData.Owner = ProcessesToStart.Owner</QueryText>
