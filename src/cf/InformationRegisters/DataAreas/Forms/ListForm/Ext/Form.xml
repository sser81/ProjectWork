			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	InformationRegisterDataAreas.DataAreaAuxiliaryData AS DataAreaAuxiliaryData,
	InformationRegisterDataAreas.Status AS Status,
	InformationRegisterDataAreas.Repeat AS Repeat,
	InformationRegisterDataAreas.RetryOnAbnormalTermination AS RetryOnAbnormalTermination,
	InformationRegisterDataAreas.DataExportedID AS DataExportedID,
	InformationRegisterDataAreas.ProcessingError AS ProcessingError,
	InformationRegisterDataAreas.Variant AS Variant,
	DataAreaPresentation.Value AS Presentation,
	DataAreaTimeZone.Value AS TimeZone,
	DataAreaPrefix.Value AS Prefix
FROM
	InformationRegister.DataAreas AS InformationRegisterDataAreas
		LEFT JOIN Constant.DataAreaPresentation AS DataAreaPresentation
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaPresentation.DataAreaAuxiliaryData
		LEFT JOIN Constant.DataAreaTimeZone AS DataAreaTimeZone
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaTimeZone.DataAreaAuxiliaryData
		LEFT JOIN Constant.DataAreaPrefix AS DataAreaPrefix
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaPrefix.DataAreaAuxiliaryData</QueryText>
