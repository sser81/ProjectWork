			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	MeasurementsStatisticsAreas.Period AS Period,
	MeasurementsStatisticsAreas.PeriodEnd AS PeriodEnd,
	MeasurementsStatisticsAreas.StatisticsOperation AS StatisticsOperation,
	StatisticsOperations.Description AS StatisticsOperationDescription,
	MeasurementsStatisticsAreas.StatisticsArea AS StatisticsArea,
	StatisticsAreas.Description AS StatisticsAreaDescription,
	MeasurementsStatisticsAreas.DeletionID AS DeletionID,
	MeasurementsStatisticsAreas.ValuesCount AS ValuesCount
FROM
	InformationRegister.MeasurementsStatisticsAreas AS MeasurementsStatisticsAreas
LEFT JOIN
	InformationRegister.StatisticsOperations AS StatisticsOperations
ON
	MeasurementsStatisticsAreas.StatisticsOperation = StatisticsOperations.OperationID
LEFT JOIN
	InformationRegister.StatisticsAreas AS StatisticsAreas
ON
	MeasurementsStatisticsAreas.StatisticsArea = StatisticsAreas.AreaID</QueryText>
