	MeasurementsStatisticsOperations.StatisticsOperation AS StatisticsOperation,
	MeasurementsStatisticsOperations.DeletionID AS DeletionID,
	MeasurementsStatisticsOperations.ValuesCount AS ValuesCount,
	MeasurementsStatisticsOperations.ValueSum AS ValueSum
