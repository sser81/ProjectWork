				<QueryText>SELECT
	InformationRegisterDocumentsRegistry.RefType AS RefType,
	InformationRegisterDocumentsRegistry.BusinessOperation AS BusinessOperation,
	InformationRegisterDocumentsRegistry.Organization AS Organization,
	InformationRegisterDocumentsRegistry.Partner AS Partner,
	InformationRegisterDocumentsRegistry.StorageLocation AS StorageLocation,
	InformationRegisterDocumentsRegistry.Counterparty AS Counterparty,
	InformationRegisterDocumentsRegistry.Department AS Department,
	InformationRegisterDocumentsRegistry.IBDocumentDate AS IBDocumentDate,
	InformationRegisterDocumentsRegistry.Ref AS Ref,
	InformationRegisterDocumentsRegistry.RecordSeparator AS RecordSeparator,
	InformationRegisterDocumentsRegistry.IBDocumentNumber AS IBDocumentNumber,
	InformationRegisterDocumentsRegistry.Status AS Status,
	InformationRegisterDocumentsRegistry.EmployeeResponsible AS EmployeeResponsible,
	InformationRegisterDocumentsRegistry.More AS More,
	InformationRegisterDocumentsRegistry.Comment AS Comment,
	InformationRegisterDocumentsRegistry.Posted AS Posted,
	InformationRegisterDocumentsRegistry.DeletionMark AS DeletionMark,
	InformationRegisterDocumentsRegistry.SourceDocumentDate AS SourceDocumentDate,
	InformationRegisterDocumentsRegistry.SourceDocumentNumber AS SourceDocumentNumber,
	InformationRegisterDocumentsRegistry.Sum AS Sum,
	InformationRegisterDocumentsRegistry.Currency AS Currency,
	InformationRegisterDocumentsRegistry.Contract AS Contract,
	InformationRegisterDocumentsRegistry.RecordingInAccountingDate AS RecordingInAccountingDate,
	CASE
		WHEN InformationRegisterDocumentsRegistry.DeletionMark
			THEN 3
		WHEN NOT InformationRegisterDocumentsRegistry.Posted
			THEN 0
		ELSE 1
	END AS RowPicture
FROM
	InformationRegister._DemoDocumentsRegistry AS InformationRegisterDocumentsRegistry</QueryText>
