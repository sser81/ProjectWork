	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.DeletionMark</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Register3.RefType,
	VALUETYPE(Register3.Ref) AS DocumentType,
	Register3.StorageLocation,
	Register3.Organization,
	Register3.Date,
	Register3.Ref,
	Register3.AdditionalRecord,
	Register3.Number,
	Register3.EmployeeResponsible,
	Register3.Comment,
	Register3.Posted,
	Register3.DeletionMark,
	CASE
		WHEN Register3.DeletionMark
			THEN 13
		WHEN Register3.Posted
			THEN 14
		ELSE 12
	END AS PictureNumber
FROM
	InformationRegister._DemoWarehouseDocumentsRegister AS Register3
{WHERE
	((Register3.Date &gt;= &amp;BeginOfPeriod
			OR &amp;BeginOfPeriod = DATETIME(1, 1, 1))
			AND (Register3.Date &lt;= &amp;EndOfPeriod
				OR &amp;EndOfPeriod = DATETIME(1, 1, 1)))}</QueryText>
				<MainTable>InformationRegister._DemoWarehouseDocumentsRegister</MainTable>
