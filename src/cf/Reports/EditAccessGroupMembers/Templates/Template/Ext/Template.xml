	<calculatedField>
		<dataPath>Used</dataPath>
		<expression>Not ProfileDeletionMark And Not AccessGroupDeletionMark And IsMemberUsed And IsUserUsed</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Used</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<totalField>
		<dataPath>MemberTill</dataPath>
		<expression>Case When Not ValueIsFilled(Min(MemberTill)) Then Min(MemberTill)
Else Max(MemberTill) End</expression>
		<group>AccessGroup</group>
		<group>User</group>
	</totalField>
	<totalField>
		<dataPath>IncludedAsGroupMember</dataPath>
		<expression>Case When Max(IsMemberUser) = True Then Undefined Else Array(Distinct IncludedAsGroupMember) End</expression>
		<group>AccessGroup</group>
		<group>User</group>
	</totalField>
	<totalField>
		<dataPath>PreviousMemberTill</dataPath>
		<expression>Case When Not ValueIsFilled(Min(PreviousMemberTill)) Then Min(PreviousMemberTill)
Else Max(PreviousMemberTill) End</expression>
		<group>AccessGroup</group>
		<group>User</group>
	</totalField>
	<totalField>
		<dataPath>Used</dataPath>
		<expression>Max(Used)</expression>
		<group>AccessGroup</group>
		<group>User</group>
	</totalField>
