		<query>SELECT
	EmployeeLimitPlaningSliceLast.Employee AS Employee,
	EmployeeLimitPlaningSliceLast.Entity AS Entity,
	EmployeeLimitPlaningSliceLast.TypeOfLimit AS TypeOfLimit,
	EmployeeLimitPlaningSliceLast.Stage AS Stage,
	EmployeeLimitPlaningSliceLast.Limit AS Limit,
	0 AS Expense,
	EmployeeLimitPlaningSliceLast.Recorder AS Recorder
INTO TemporaryTableData
FROM
	InformationRegister.EmployeeLimitPlaning.SliceLast(
			&amp;ReportDate,
			(TypeOfLimit.AmountLimitControl
				OR TypeOfLimit.CountLimitControl)
				AND (TypeOfLimit.OneTime
					OR Stage &lt;&gt; &amp;StageEmptyRef) {(Entity).*, (Employee).*, (TypeOfLimit).*, (Stage).*}) AS EmployeeLimitPlaningSliceLast

UNION ALL

SELECT
	TurnoverByEmployeeLimitsTurnovers.Employee,
	TurnoverByEmployeeLimitsTurnovers.Entity,
	TurnoverByEmployeeLimitsTurnovers.TypeOfLimit,
	TurnoverByEmployeeLimitsTurnovers.Stage,
	0,
	TurnoverByEmployeeLimitsTurnovers.LimitTurnover,
	TurnoverByEmployeeLimitsTurnovers.Recorder.Ref
FROM
	AccumulationRegister.TurnoverByEmployeeLimits.Turnovers(
			,
			ENDOFPERIOD(&amp;ReportDate, DAY) {(&amp;ReportDate)},
			Auto,
			(TypeOfLimit.AmountLimitControl
				OR TypeOfLimit.CountLimitControl)
				AND (TypeOfLimit.OneTime
					OR Stage &lt;&gt; &amp;StageEmptyRef) {(Entity).*, (Employee).*, (TypeOfLimit).*, (Stage).*}) AS TurnoverByEmployeeLimitsTurnovers
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TemporaryTableData.Employee AS Employee,
	TemporaryTableData.Entity AS Entity,
	TemporaryTableData.TypeOfLimit AS TypeOfLimit,
	TemporaryTableData.Stage AS Stage,
	CAST(EmployeesSliceFirst.Recorder AS Document.EmploymentContract) AS EmploymentContract,
	UNDEFINED AS EmploymentContractChange,
	TemporaryTableData.Limit AS Limit,
	TemporaryTableData.Expense AS Expense,
	FALSE AS Treminated,
	EmployeesSliceFirst.Department_Company AS Department,
	TemporaryTableData.Recorder AS Recorder
{SELECT
	Employee.*,
	Entity.*,
	Department,
	TypeOfLimit.*,
	Stage.*,
	Limit,
	Expense,
	Treminated,
	EmploymentContractChange,
	EmploymentContract.*,
	Recorder.*}
FROM
	TemporaryTableData AS TemporaryTableData
		LEFT JOIN InformationRegister._DemoCompaniesEmployees.SliceFirst(, {(Organization) AS Entity, (Individual) AS Employee}) AS EmployeesSliceFirst
		ON TemporaryTableData.Entity = EmployeesSliceFirst.Organization
			AND TemporaryTableData.Employee = EmployeesSliceFirst.Individual
WHERE
	(TemporaryTableData.Limit &lt;&gt; 0
			OR TemporaryTableData.Expense &lt;&gt; 0)</query>
