			<dataPath>Size</dataPath>
			<field>Size</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Extension</dataPath>
			<field>Extension</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Ref</dataPath>
			<field>Ref</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	BEGINOFPERIOD(FilesVersions.FileModificationDate, DAY) AS Period,
	FilesVersions.Ref AS Ref,
	COUNT(DISTINCT FilesVersions.Ref) AS Count,
	FilesVersions.Size AS Size,
	FilesVersions.Extension,
	FilesVersions.Volume,
	FilesVersions.Author
FROM
	Catalog.FilesVersions AS FilesVersions
{WHERE
	(FilesVersions.FileModificationDate BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod)}

GROUP BY
	FilesVersions.Ref,
	FilesVersions.Size,
	FilesVersions.Extension,
	FilesVersions.Volume,
	FilesVersions.Author,
	BEGINOFPERIOD(FilesVersions.FileModificationDate, DAY)</query>
	</dataSet>
	<totalField>
		<dataPath>Count</dataPath>
		<expression>Sum(Count)</expression>
	</totalField>
