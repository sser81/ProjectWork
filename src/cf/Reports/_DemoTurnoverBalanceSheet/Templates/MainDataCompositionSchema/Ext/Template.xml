		<dataSource>DataSource1</dataSource>
		<query>SELECT
	BalanceAndTurnovers.Account AS Account,
	
	BalanceAndTurnovers.Organization AS Organization,
	BalanceAndTurnovers.Currency      AS Currency,
	BalanceAndTurnovers.Recorder AS Document,
	
	BalanceAndTurnovers.SumOpeningBalanceDr AS AmountOfInitialDr,
	BalanceAndTurnovers.SumOpeningBalanceCr AS AmountOfInitialCr,
	BalanceAndTurnovers.SumTurnoverDr           AS SumTurnoverDr,
	BalanceAndTurnovers.SumTurnoverCr           AS SumTurnoverCr,
	BalanceAndTurnovers.SumClosingBalanceDr  AS SumOfConOstDr,
	BalanceAndTurnovers.SumClosingBalanceCr  AS SumOfConOstCr,
	
	BalanceAndTurnovers.CountOpeningBalanceDr AS OpeningBalanceDrCount,
	BalanceAndTurnovers.CountOpeningBalanceCr AS OpeningBalanceCrCount,
	BalanceAndTurnovers.CountTurnoverDr           AS CountTurnoverDr,
	BalanceAndTurnovers.CountTurnoverCr           AS CountTurnoverCr,
	BalanceAndTurnovers.CountClosingBalanceDr  AS CloseBalanceDrCount,
	BalanceAndTurnovers.CountClosingBalanceCr  AS CloseBalanceCrCount,
	
	BalanceAndTurnovers.CurrencyAmountOpeningBalanceDr AS CurrencyAmountOpenBalDr,
	BalanceAndTurnovers.CurrencyAmountOpeningBalanceCr AS CurrencyAmountOpenBalCr,
	BalanceAndTurnovers.CurrencyAmountTurnoverDr           AS CurrencyAmountTurnoverDr,
	BalanceAndTurnovers.CurrencyAmountTurnoverCr           AS CurrencyAmountTurnoverCr,
	BalanceAndTurnovers.CurrencyAmountClosingBalanceDr  AS CurrencyAmountClosBalDr,
	BalanceAndTurnovers.CurrencyAmountClosingBalanceCr  AS CurrencyAmountClosBalCr
	
FROM
	AccountingRegister._DemoAccountingTransactionLog.BalanceAndTurnovers(
		{&amp;BeginOfPeriod}, {&amp;EndOfPeriod}, Auto, ,
		{Account IN HIERARCHY(&amp;Account)},
		,
		{Organization = &amp;Organization} {Currency = &amp;Currency }
	) AS BalanceAndTurnovers
	
</query>
