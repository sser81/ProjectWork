	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.AcceptedForExecution</Field>
				<Field>List.BusinessProcess</Field>
				<Field>List.Ref</Field>
				<Field>List.SubjectOf</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	TasksByExecutive.Ref,
	TasksByExecutive.DeletionMark,
	TasksByExecutive.Number,
	TasksByExecutive.Date,
	TasksByExecutive.BusinessProcess,
	TasksByExecutive.RoutePoint,
	TasksByExecutive.Description,
	TasksByExecutive.Executed,
	TasksByExecutive.TaskDueDate,
	CASE
		WHEN TasksByExecutive.StartDate = &amp;BlankDate
			THEN &amp;BegOfDay
		ELSE TasksByExecutive.StartDate
	END AS StartDate,
	TasksByExecutive.CompletionDate,
	TasksByExecutive.Importance,
	TasksByExecutive.LongDesc,
	TasksByExecutive.PerformerRole,
	TasksByExecutive.Performer,
	TasksByExecutive.Presentation,
	TasksByExecutive.SubjectString,
	TasksByExecutive.Author,
	TasksByExecutive.AcceptedForExecution,
	CASE
		WHEN TasksByExecutive.Importance = VALUE(Enum.TaskImportanceOptions.Low)
			THEN 0
		WHEN TasksByExecutive.Importance = VALUE(Enum.TaskImportanceOptions.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	TasksByExecutive.SubjectOf,
	CASE
		WHEN TasksByExecutive.TaskDueDate &lt;= &amp;CurrentDate
			THEN &amp;Overdue
		WHEN TasksByExecutive.TaskDueDate &gt; &amp;CurrentDate AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfDay
			THEN &amp;Today
		WHEN TasksByExecutive.TaskDueDate &gt; &amp;EndOfDay AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfWeek
			THEN &amp;ThisWeek
		WHEN TasksByExecutive.TaskDueDate &gt; &amp;EndOfWeek AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfNextWeek
			THEN &amp;NextWeek
		ELSE &amp;Later
	END AS GroupDueDate
FROM
	Task.PerformerTask.TasksByExecutive AS TasksByExecutive
WHERE
	TasksByExecutive.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Running)</QueryText>
				<MainTable>Task.PerformerTask.TasksByExecutive</MainTable>
